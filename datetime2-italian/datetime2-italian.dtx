%
% Define the \texttt{italian} style.
% The time style is the same as the "default" style
% provided by \sty{datetime2}. This may need correcting. 
%
% Allow the user a way of configuring the "italian" and
% "italian-numeric" styles. This doesn't use the package wide
% separators such as
% \cs{dtm@datetimesep} in case other date formats are also required.
%\begin{macro}{\DTMITdaymonthsep}
% The separator between the day and month for the text format.
%    \begin{macrocode}
\newcommand*{\DTMITdaymonthsep}{\space}
%    \end{macrocode}
%\end{macro}
%
%\begin{macro}{\DTMITmonthyearsep}
% The separator between the month and year for the text format.
%    \begin{macrocode}
\newcommand*{\DTMITmonthyearsep}{\space}
%    \end{macrocode}
%\end{macro}
%
%\begin{macro}{\DTMITdatetimesep}
% The separator between the date and time blocks in the full format
% (either text or numeric).
%    \begin{macrocode}
\newcommand*{\DTMITdatetimesep}{\space}
%    \end{macrocode}
%\end{macro}
%
%\begin{macro}{\DTMITtimezonesep}
% The separator between the time and zone blocks in the full format
% (either text or numeric).
%    \begin{macrocode}
\newcommand*{\DTMITtimezonesep}{\space}
%    \end{macrocode}
%\end{macro}
%
%\begin{macro}{\DTMITdatesep}
% The separator for the numeric date format.
%    \begin{macrocode}
\newcommand*{\DTMITdatesep}{/}
%    \end{macrocode}
%\end{macro}
%
%\begin{macro}{\DTMITtimesep}
% The separator for the numeric time format.
%    \begin{macrocode}
\newcommand*{\DTMITtimesep}{:}
%    \end{macrocode}
%\end{macro}
%
%Provide keys that can be used in \cs{DTMlangsetup} to set these
%separators.
%    \begin{macrocode}
\DTMdefkey{italian}{daymonthsep}{\renewcommand*{\DTMITdaymonthsep}{#1}}
\DTMdefkey{italian}{monthyearsep}{\renewcommand*{\DTMITmonthyearsep}{#1}}
\DTMdefkey{italian}{datetimesep}{\renewcommand*{\DTMITdatetimesep}{#1}}
\DTMdefkey{italian}{timezonesep}{\renewcommand*{\DTMITtimezonesep}{#1}}
\DTMdefkey{italian}{datesep}{\renewcommand*{\DTMITdatesep}{#1}}
\DTMdefkey{italian}{timesep}{\renewcommand*{\DTMITtimesep}{#1}}
%    \end{macrocode}
%
% Define a boolean key to switch between full and abbreviated
% formats
%    \begin{macrocode}
\DTMdefboolkey{italian}{abbr}[true]{}
%    \end{macrocode}
% The default is 
%    \begin{macrocode}
\DTMsetbool{italian}{abbr}{false}
%    \end{macrocode}
%
% Define a boolean key that determines if the time zone mappings
% should be used.
%    \begin{macrocode}
\DTMdefboolkey{italian}{mapzone}[true]{}
%    \end{macrocode}
% The default is to use mappings.
%    \begin{macrocode}
\DTMsetbool{italian}{mapzone}{false}
%    \end{macrocode}
%
% Define a boolean key that determines if the day of month should be
% displayed.
%    \begin{macrocode}
\DTMdefboolkey{italian}{showdayofmonth}[true]{}
%    \end{macrocode}
% The default is to show the day of month.
%    \begin{macrocode}
\DTMsetbool{italian}{showdayofmonth}{true}
%    \end{macrocode}
%
% Define a boolean key that determines if the year should be
% displayed.
%    \begin{macrocode}
\DTMdefboolkey{italian}{showyear}[true]{}
%    \end{macrocode}
% The default is to show the year.
%    \begin{macrocode}
\DTMsetbool{italian}{showyear}{true}
%    \end{macrocode}
%\begin{macro}{\DTMITfmtordsuffix}
% The separator for the numeric time format.
%    \begin{macrocode}
\newcommand*{\DTMITfmtordsuffix}[1]{}
\DTMdefchoicekey{italian}{ord}[\val\nr]{level,raise,omit,sc}{%
	\ifcase\nr\relax
	\renewcommand*{\DTMITfmtordsuffix}[1]{##1}%
	\or
	\renewcommand*{\DTMITfmtordsuffix}[1]{%
		\DTMtexorpdfstring{\protect\textsuperscript{##1}}{##1}}%
	\or
	\renewcommand*{\DTMITfmtordsuffix}[1]{}%
	\or
	\renewcommand*{\DTMITfmtordsuffix}[1]{%
		\DTMtexorpdfstring{\protect\textsc{##1}}{##1}}%
	\fi
}
%    \end{macrocode}
%\end{macro}
% Define the "italian" style. (TODO: implement day of week?)
%    \begin{macrocode}
\DTMnewstyle
{italian}% label
{% date style
	\renewcommand*{\DTMitalianfmtordsuffix}{\DTMITfmtordsuffix}%
	\renewcommand*\DTMdisplaydate[4]{%
		\ifDTMshowdow
		\ifnum##4>-1%
		\DTMifbool{italian}{abbr}%
		{\DTMitalianshortweekdayname{##4}}%
		{\DTMitalianweekdayname{##4}}%
		\space
		\fi
		\fi
		\DTMifbool{italian}{showdayofmonth}%
		{%
			\DTMitalianordinal{##3}%
			\DTMITdaymonthsep
		}%
		{}%
		\DTMifbool{italian}{abbr}%
		{\DTMitalianshortmonthname{##2}}%
		{\DTMitalianmonthname{##2}}%
		\DTMifbool{italian}{showyear}%
		{%
			\DTMITmonthyearsep\number##1 % space intended
		}%
		{}%
	}%
	\renewcommand*\DTMDisplaydate[4]{%
		\ifDTMshowdow
		\ifnum##4>-1%
		\DTMifbool{italian}{abbr}%
		{\DTMitalianshortweekdayname{##4}}%
		{\DTMitalianweekdayname{##4}}%
		\space
		\fi
		\fi
		\DTMifbool{italian}{showdayofmonth}%
		{%
			\DTMitalianordinal{##3}%
			\DTMITdaymonthsep
		}%
		{}%
		\DTMifbool{italian}{abbr}%
		{\DTMitalianshortmonthname{##2}}%
		{\DTMitalianmonthname{##2}}%
		\DTMifbool{italian}{showyear}%
		{%
			\DTMITmonthyearsep\number##1 % space intended
		}%
		{}%
	}%
}%
{% time style
	\renewcommand*\DTMitaliantimesep{\DTMITtimesep}%
	\DTMsettimestyle{italianampm}%
}%
{% zone style
	\DTMresetzones
	\DTMITzonemaps
	\renewcommand*{\DTMdisplayzone}[2]{%
		\DTMifbool{italian}{mapzone}%
		{\DTMusezonemapordefault{##1}{##2}}%
		{%
			\ifnum##1<0\else+\fi\DTMtwodigits{##1}%
			\ifDTMshowzoneminutes\DTMITtimesep\DTMtwodigits{##2}\fi
		}%
	}%
}%
{% full style
	\renewcommand*{\DTMdisplay}[9]{%
		\ifDTMshowdate
		\DTMdisplaydate{##1}{##2}{##3}{##4}%
		\DTMITdatetimesep
		\fi
		\DTMdisplaytime{##5}{##6}{##7}%
		\ifDTMshowzone
		\DTMITtimezonesep
		\DTMdisplayzone{##8}{##9}%
		\fi
	}%
	\renewcommand*{\DTMDisplay}[9]{%
		\ifDTMshowdate
		\DTMDisplaydate{##1}{##2}{##3}{##4}%
		\DTMITdatetimesep
		\fi
		\DTMdisplaytime{##5}{##6}{##7}%
		\ifDTMshowzone
		\DTMITtimezonesep
		\DTMdisplayzone{##8}{##9}%
		\fi
	}%
}%
%    \end{macrocode}
%
% Define numeric style.
%    \begin{macrocode}
\DTMnewstyle
{italian-numeric}% label
{% date style
	\renewcommand*\DTMdisplaydate[4]{%
		\DTMifbool{italian}{showdayofmonth}%
		{%
			\number##3 % space intended
			\DTMITdatesep
		}%
		{}%
		\number##2 % space intended
		\DTMifbool{italian}{showyear}%
		{%
			\DTMITdatesep
			\number##1 % space intended
		}%
		{}%
	}%
	\renewcommand*\DTMDisplaydate[4]{%
		\DTMifbool{italian}{showdayofmonth}
		{%
			\DTMitalianordinal{##3}\DTMitaliandaymonthsep
			\DTMitalianmonthname{##2}%
		}%
		{\DTMitalianmonthname{##2}}%
		\DTMifbool{italian}{showyear}%
		{%
			\DTMitalianmonthyearsep
			\number##1 % space intended
		}%
		{}%
	}%
}%
