% Redefine \cs{dateitalian} (or \cs{date}\meta{dialect}) to prevent
% \sty{babel} from resetting \cs{today}. (For this to work,
% \sty{babel} must already have been loaded if it's required.)
%    \begin{macrocode}
\ifcsundef{date\CurrentTrackedDialect}
{% do nothing
	\ifundef\dateitalian
	{%
	}%
	{%
		\def\dateitalian{%
			\DTMifcaseregional
			{}% do nothing
			{\DTMsetstyle{italian}}%
			{\DTMsetstyle{italian-numeric}}%
		}%
	}%
}%
{%
	\csdef{date\CurrentTrackedDialect}{%
		\DTMifcaseregional
		{}% do nothing
		{\DTMsetstyle{italian}}%
		{\DTMsetstyle{italian-regional}}%
	}%
}%
%    \end{macrocode}
%\iffalse
